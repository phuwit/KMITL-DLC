	"version": "0.2.0",
	"type": "module",
	"scripts": {
		"sign": "mkdir -p temp && git archive --format=zip --output=temp/source.zip HEAD && dotenv -- web-ext sign --source-dir dist/ --channel unlisted --upload-source-code temp/source.zip",
		"sign:build": "npm run build && npm run sign",
		"dev": "wxt",
		"dev:firefox": "wxt -b firefox",
		"build": "wxt build",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:firefox": "wxt zip -b firefox",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"postinstall": "wxt prepare",
		"format": "prettier --write .",
		"lint": "prettier --check .; eslint .; web-ext lint"
	},
